<p id="notice"><%= notice %></p>

<h1>Timetickets</h1>

<table>
  <thead>
    <tr>
      <th>Banana</th>
      <th>Time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @timetickets.each do |timeticket| %>
      <tr>
        <td><%= timeticket.banana %></td>
        <td><%= timeticket.time %></td>
        <td><%= link_to 'Show', timeticket %></td>
        <td><%= link_to 'Edit', edit_timeticket_path(timeticket) %></td>
        <td><%= link_to 'Destroy', timeticket, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Timeticket', new_timeticket_path %>
